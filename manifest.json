{
  "manifest_version": 2,

  "name": "AddSomeBlack",
  "description": "This extension paints page content in black",
  "version": "0.2",
  "icons": {
    "48": "icons/liquid-droplet-48.png",
    "96": "icons/liquid-droplet-96.png"
  },
  "browser_action": {
    "default_icon": "icons/liquid-droplet-48.png",
    "default_popup": "popup.html",
    "default_title": "AddSomeBlack"
  },
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}